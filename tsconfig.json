{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "target": "esnext",
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": [
      "jest",
      "react",
      "react-native"
    ]
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "Pods"]
}
